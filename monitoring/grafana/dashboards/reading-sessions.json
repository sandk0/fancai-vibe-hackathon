{
  "id": null,
  "uid": "reading-sessions-v1",
  "title": "BookReader AI - Reading Sessions Monitoring",
  "tags": [
    "reading-sessions",
    "bookreader",
    "user-activity"
  ],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "type": "stat",
      "title": "Active Sessions",
      "targets": [
        {
          "expr": "sum(reading_sessions_active_count)",
          "refId": "A",
          "legendFormat": "Active Sessions"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "justifyMode": "center",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 500,
                "color": "yellow"
              },
              {
                "value": 1000,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "type": "stat",
      "title": "Concurrent Users",
      "targets": [
        {
          "expr": "reading_sessions_concurrent_users",
          "refId": "A",
          "legendFormat": "Concurrent Users"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "justifyMode": "center"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 100,
                "color": "yellow"
              },
              {
                "value": 500,
                "color": "orange"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "type": "stat",
      "title": "Abandoned Sessions",
      "targets": [
        {
          "expr": "reading_sessions_abandoned_count",
          "refId": "A",
          "legendFormat": "Abandoned (>24h)"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "justifyMode": "center"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 100,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "type": "stat",
      "title": "Error Rate (5m)",
      "targets": [
        {
          "expr": "rate(reading_sessions_errors_total[5m])",
          "refId": "A",
          "legendFormat": "Errors/sec"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "justifyMode": "center"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 0.01,
                "color": "yellow"
              },
              {
                "value": 0.05,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "type": "graph",
      "title": "Sessions Started vs Ended",
      "targets": [
        {
          "expr": "rate(reading_sessions_started_total[5m])",
          "refId": "A",
          "legendFormat": "Started ({{ device_type }})"
        },
        {
          "expr": "rate(reading_sessions_ended_total[5m])",
          "refId": "B",
          "legendFormat": "Ended ({{ completion_status }})"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "ops",
          "label": "Sessions/sec"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true,
        "max": true
      }
    },
    {
      "id": 6,
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "type": "graph",
      "title": "Active Sessions Over Time",
      "targets": [
        {
          "expr": "sum by(device_type) (reading_sessions_active_count)",
          "refId": "A",
          "legendFormat": "{{ device_type }}"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "short",
          "label": "Active Sessions"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true,
        "max": true
      },
      "fill": 2,
      "stack": true
    },
    {
      "id": 7,
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 17
      },
      "type": "graph",
      "title": "Session Duration (p50, p95, p99)",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(reading_session_duration_seconds_bucket[5m]))",
          "refId": "A",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(reading_session_duration_seconds_bucket[5m]))",
          "refId": "B",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(reading_session_duration_seconds_bucket[5m]))",
          "refId": "C",
          "legendFormat": "p99"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "s",
          "label": "Duration"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true,
        "max": true
      }
    },
    {
      "id": 8,
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 17
      },
      "type": "graph",
      "title": "Average Pages Read per Session",
      "targets": [
        {
          "expr": "rate(reading_session_pages_read_sum[5m]) / rate(reading_session_pages_read_count[5m])",
          "refId": "A",
          "legendFormat": "{{ device_type }}"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "short",
          "label": "Pages"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true
      }
    },
    {
      "id": 9,
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 26
      },
      "type": "piechart",
      "title": "Device Type Distribution",
      "targets": [
        {
          "expr": "sum by(device_type) (reading_sessions_started_total)",
          "refId": "A",
          "legendFormat": "{{ device_type }}"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "pieType": "pie",
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "value",
            "percent"
          ]
        }
      }
    },
    {
      "id": 10,
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 26
      },
      "type": "piechart",
      "title": "Session Completion Status",
      "targets": [
        {
          "expr": "sum by(completion_status) (reading_sessions_ended_total)",
          "refId": "A",
          "legendFormat": "{{ completion_status }}"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "pieType": "donut",
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "value",
            "percent"
          ]
        }
      }
    },
    {
      "id": 11,
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 26
      },
      "type": "stat",
      "title": "API Latency p95 (5m)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(reading_session_api_latency_seconds_bucket[5m]))",
          "refId": "A",
          "legendFormat": "{{ endpoint }}"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "justifyMode": "center"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 0.5,
                "color": "yellow"
              },
              {
                "value": 1.0,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 12,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 34
      },
      "type": "table",
      "title": "Error Breakdown by Operation",
      "targets": [
        {
          "expr": "sum by(operation, error_type) (reading_sessions_errors_total)",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "options": {
        "showHeader": true
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        }
      }
    },
    {
      "id": 13,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 34
      },
      "type": "graph",
      "title": "Session Updates per Minute",
      "targets": [
        {
          "expr": "rate(reading_sessions_updated_total[1m]) * 60",
          "refId": "A",
          "legendFormat": "{{ device_type }}"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "short",
          "label": "Updates/min"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "text": "Prometheus",
          "value": "Prometheus"
        }
      },
      {
        "name": "device_type",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(reading_sessions_started_total, device_type)",
        "multi": true,
        "includeAll": true,
        "refresh": 1
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "datasource": "Prometheus",
        "enable": true,
        "expr": "ALERTS{alertname=~\".*ReadingSession.*\", alertstate=\"firing\"}",
        "iconColor": "red",
        "name": "Reading Sessions Alerts",
        "tagKeys": "severity,component"
      }
    ]
  },
  "__inputs": [],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "9.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ]
}